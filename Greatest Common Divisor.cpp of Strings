#include <iostream>
#include <string>
using namespace std;

// Helper function to find GCD of two numbers
int gcd(int a, int b) {
    return b == 0 ? a : gcd(b, a % b);
}

string gcdOfStrings(string str1, string str2) {
    // If concatenation in both orders isn't equal, no common pattern
    if (str1 + str2 != str2 + str1)
        return "";

    // GCD of lengths gives us the size of the repeating pattern
    int len = gcd(str1.size(), str2.size());
    return str1.substr(0, len);
}

int main() {
    string str1, str2;
    cout << "Enter first string: ";
    cin >> str1;
    cout << "Enter second string: ";
    cin >> str2;

    cout << "GCD of Strings: " << gcdOfStrings(str1, str2) << endl;
    return 0;
}
